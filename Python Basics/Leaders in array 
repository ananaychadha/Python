def printLeaders(arr,size): 
      
    for i in range(0, size): 
        for j in range(i+1, size): 
          
            if arr[i]<arr[j]: 
                break
        if arr[i]>=arr[j] :  
            print (arr[i],end = " ")



n= int(input())
arr = [int(x) for x in input().split()]
arr =printLeaders(arr, n)