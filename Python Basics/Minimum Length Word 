string=input()
word = "";  
words = [];  
   

string = string + " ";  
   
for i in range(0, len(string)):  
   
    if(string[i] != ' '):  
        word = word + string[i];  
    else:  
          
        words.append(word);  
    
        word = "";  
          

small = words[0];  
   

for k in range(0, len(words)):  
      
     
    if(len(small) > len(words[k])):  
        small = words[k];  
          
     
     
   
print(small);  
 